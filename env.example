# LLM Provider API Keys
# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI (GPT)
OPENAI_API_KEY=your_openai_api_key_here

# Google (Gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration
# For Local Adapter
DATABASE_PATH=./data/wukong.db

# For Vercel Adapter
POSTGRES_URL=your_postgres_connection_url_here
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here
POSTGRES_URL_NON_POOLING=your_postgres_non_pooling_url_here

# Vercel Blob Storage (for Vercel Adapter)
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Vercel KV Storage (for Vercel Adapter)
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_read_only_token_here

# Embeddings Configuration (optional)
EMBEDDINGS_MODEL=text-embedding-ada-002
EMBEDDINGS_DIMENSIONS=1536

# Agent Configuration (optional)
AGENT_MAX_ITERATIONS=10
AGENT_TIMEOUT_MS=30000

# Environment
NODE_ENV=development


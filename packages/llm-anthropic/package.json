{
  "name": "@wukong/llm-anthropic",
  "version": "0.1.0",
  "description": "Anthropic Claude LLM adapter for Wukong",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@wukong/agent": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["anthropic", "claude", "llm", "wukong"],
  "license": "MIT"
}

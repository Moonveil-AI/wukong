{
  "name": "@wukong/client",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript client for Wukong server",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["wukong", "client", "http", "websocket", "sse", "api"],
  "license": "MIT"
}

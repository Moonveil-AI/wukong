{
  "name": "@wukong/adapter-vercel",
  "version": "0.1.0",
  "description": "Vercel adapter for Wukong (Postgres + KV + Blob)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "migrations"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@wukong/agent": "workspace:*",
    "@vercel/postgres": "^0.10.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/blob": "^0.27.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["vercel", "postgres", "adapter", "wukong"],
  "license": "MIT"
}

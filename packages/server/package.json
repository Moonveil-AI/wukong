{
  "name": "@wukong/server",
  "version": "0.1.0",
  "description": "Production-ready backend server for Wukong agent execution",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@wukong/agent": "workspace:*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "ws": "^8.16.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0"
  },
  "keywords": ["wukong", "agent", "server", "express", "websocket", "sse"],
  "author": "Wukong Team",
  "license": "MIT"
}

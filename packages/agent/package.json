{
  "name": "@wukong/agent",
  "version": "0.1.0",
  "description": "Core Wukong agent library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eventemitter3": "^5.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "agent",
    "ai",
    "llm",
    "typescript",
    "wukong"
  ],
  "license": "MIT"
}
